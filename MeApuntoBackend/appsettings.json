{
  "Logging": {
    "LogFilePath": "Logs\\log-{Date}.txt",
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "Debug": { // Debug logging provider.
    "LogLevel": {
      "Default": "Error" // Overrides preceding LogLevel:Default setting.
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=DB/db_production.db"
  }
}
